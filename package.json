{
  "author": "Chris Nielsen <nielsen.chris@gmail.com>",
  "dependencies": {
    "ajv": "^8.12.0",
    "ansi-colors": "^4.1.3",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "fluent-json-schema": "^4.1.0",
    "json-schema-to-typescript": "^13.0.1",
    "prettier": "^2.8.8",
    "tslib": "^2.5.2"
  },
  "devDependencies": {
    "@types/json-schema": "7.X.X",
    "@types/node": "18.X.X",
    "@types/prettier": "2.X.X",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "typescript": "^5.0.4",
    "undici": "^5.22.1"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "name": "contentstack-type-gen",
  "private": false,
  "scripts": {
    "build": "tsc -b ./tsconfig.json",
    "clean": "rm -rf ./.jest ./dist ./tsconfig.src.tsbuildinfo ./src/generate/*.d.ts",
    "lint": "eslint .",
    "postbuild": "cp src/generate/*.schema.json dist/generate",
    "prebuild": "json2ts -i src/generate/GetAllContentTypesResponse.schema.json -o src/generate/IGetAllContentTypesResponse.d.ts",
    "pretest": "yarn build",
    "pretty": "prettier . --check",
    "test": "node --experimental-vm-modules --enable-source-maps --no-warnings $(yarn bin jest) --config .jest/dist/jest.config.js"
  },
  "type": "module",
  "version": "1.0.0"
}
