    "globals": "^15.4.0",
    "jest": "^29.7.0",
    "typescript": "^5.4.5"
  },
  "imports": {
    "#src/*.js": "./dist/*.js"
  },
  "license": "MIT",
  "main": "dist/cs-gen.js",
  "name": "@altearius/cs-gen",
  "packageManager": "yarn@4.2.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/altearius/cs-gen"
